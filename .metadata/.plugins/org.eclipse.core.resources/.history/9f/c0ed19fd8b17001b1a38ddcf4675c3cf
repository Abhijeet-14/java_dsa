package _java;

import java.util.*;


public class ChefandLaddusForChildren {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int t = sc.nextInt();
		
		while(t-->0) {
			int n = sc.nextInt();
			int k = sc.nextInt();
			int arr[] = new int[n];
			
			int mxx = Integer.MIN_VALUE;
			for(int i=0; i<n; i++) {
				arr[i] = sc.nextInt();
				if(mxx<arr[i]) {
					mxx = arr[i];
				}
			}
			
			int mx = subset(n,k, arr, mxx);
			System.out.println(mx);
		}
	}
	
	static int subset(int n, int k, int arr[], int sum) {
		if(k<=1 || n<=1)
			return 0;
		
		if(arr[n-1]<= sum)
			return Math.max(subset(n-1, k-1, arr, sum),subset(n, k-1, arr, sum));
		else
			return subset(n, k-1, arr, sum);
	}

}
