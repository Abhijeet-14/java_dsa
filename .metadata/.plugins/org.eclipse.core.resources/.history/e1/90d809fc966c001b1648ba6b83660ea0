package OOPS;

public class MainClass {

	public static void main(String[] args) {
		Person p1 = new Person();
		p1.age = 24;
		p1.name = "Anuj";
		
		Person p2 = new Person();
		p2.age = 23;
		p2.name = "Abhijeet";
		
		System.out.println(p1.name + " " + p1.age  );
		System.out.println(p2.name + " " + p2.age  );
	}

}


class Person{
	String name;
	int age;
}