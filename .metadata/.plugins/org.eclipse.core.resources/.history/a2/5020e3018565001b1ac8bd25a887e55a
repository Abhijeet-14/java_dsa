package _java._4Dynamic_Programing;

public class C8_Target_Sum {
	int arr[] = {1,1,2,3};
	
	int sum = 1;
	
	int n = arr.length;
	
	/*
	 * Here, we need to give -/+ to each element so that we get sum = 1
	 * 
	 * so.. we can say.. positive subset = S1 & negative subset = S2
	 * 
	 * So, S1-S2 = sum
	 * 	   S1+S2 = sum_of_array
	 *
	 *	S1 = (sum + sum_of_array)/2
	 */
	
	int t[][] = new int[n+1][sum+1];
