package _java.__Collection_FramWork._5Tree;

public class _4MainBST {

	public static void main(String[] args) {

		_3BinarySearchTree bst = new _3BinarySearchTree();
		
		bst.add(30);
		bst.add(20);
		bst.add(25);
		bst.add(15);
		bst.add(5);
		bst.add(10);
		bst.add(40);
		bst.add(35);
		bst.add(45);
		bst.add(55);
		
		
		bst.inOrder();
		System.out.println();
		
		bst.preOrder();
		System.out.println();

		bst.postOrder();
		System.out.println();
	}

}
