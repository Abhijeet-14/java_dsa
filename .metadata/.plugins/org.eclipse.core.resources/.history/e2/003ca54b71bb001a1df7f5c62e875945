package _java.__Collection_FramWork._5Tree;

public class _4MainBST {

	public static void main(String[] args) {

		_3BinarySearchTree bst = new _3BinarySearchTree();
		
		bst.insert(30);
		bst.insert(20);
		bst.insert(25);
		bst.insert(15);
		bst.insert(10);
		bst.insert(5);
		
		bst.insert(-1);
		
		bst.insert(40);
		bst.insert(35);
		bst.insert(45);
		bst.insert(55);
		
		
		bst.inOrder();
		System.out.println();
		
		bst.preOrder();
		System.out.println();

		bst.postOrder();
		System.out.println();
	}

}
